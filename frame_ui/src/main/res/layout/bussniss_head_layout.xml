<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.ContentFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <android.support.v7.widget.LinearLayoutCompat
        android:id="@+id/bussniss_nodata_linear"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:orientation="vertical"
        >
        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:visibility="gone"
            android:paddingTop="25dp"
            android:background="#37BBFB"
            >


            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                >
                <android.support.v7.widget.ContentFrameLayout
                    android:id="@+id/busniss_person_frame"
                    android:layout_width="16dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:background="@drawable/busniss_shape">


                    <android.support.v7.widget.AppCompatImageView
                        android:id="@+id/busniss_person_image"
                        android:layout_width="16dp"
                        android:layout_gravity="center|bottom"
                        android:layout_height="49dp"
                        android:background="@drawable/bussniss_item_shape"
                        />

                </android.support.v7.widget.ContentFrameLayout>


            </android.support.v7.widget.LinearLayoutCompat>


            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                >
                <android.support.v7.widget.ContentFrameLayout
                    android:id="@+id/busniss_team_frame"
                    android:layout_width="16dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:background="@drawable/busniss_shape"

                    >
                    <android.support.v7.widget.AppCompatImageView
                        android:id="@+id/busniss_team_image"
                        android:layout_width="16dp"
                        android:layout_gravity="center|bottom"
                        android:layout_height="88dp"
                        android:background="@drawable/bussniss_item_shape"
                        />
                </android.support.v7.widget.ContentFrameLayout>



            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                >
                <android.support.v7.widget.ContentFrameLayout
                    android:id="@+id/busniss_follow_frame"
                    android:layout_width="16dp"
                    android:layout_height="88dp"
                    android:layout_gravity="center"
                    android:background="@drawable/busniss_shape"

                    >
                    <android.support.v7.widget.AppCompatImageView
                        android:id="@+id/busniss_follow_image"
                        android:layout_width="16dp"
                        android:layout_gravity="center|bottom"
                        android:layout_height="36dp"
                        android:background="@drawable/bussniss_item_shape"
                        />

                </android.support.v7.widget.ContentFrameLayout>


            </android.support.v7.widget.LinearLayoutCompat>
        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:id="@+id/bussess_head_linear1"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:orientation="horizontal"
            android:background="#37BBFB"
            >


            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:layout_weight="1"
                >

                <microtech.hxswork.com.frame_ui.widget.CountNumberView
                    android:id="@+id/bissniss_personal_number"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="20"
                    android:layout_gravity="center"
                    android:textSize="30sp"
                    android:textColor="#ffffff"
                    />

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="#F0F5F7"
                    android:textSize="12sp"
                    android:text="个人签约"
                    />
            </android.support.v7.widget.LinearLayoutCompat>


            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                >

                <microtech.hxswork.com.frame_ui.widget.CountNumberView
                    android:id="@+id/busniss_team_number"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="198"
                    android:layout_gravity="center"
                    android:textSize="30sp"
                    android:textColor="#ffffff"
                    />

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="#F0F5F7"
                    android:textSize="12sp"
                    android:text="团队签约"
                    />

            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                >
                <microtech.hxswork.com.frame_ui.widget.CountNumberView
                    android:id="@+id/busniss_follow_number"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="56%"
                    android:layout_gravity="center"
                    android:textSize="30sp"
                    android:textColor="#ffffff"
                    />

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="#F0F5F7"
                    android:textSize="12sp"
                    android:text="随访率"
                    />

            </android.support.v7.widget.LinearLayoutCompat>
        </android.support.v7.widget.LinearLayoutCompat>
    </android.support.v7.widget.LinearLayoutCompat>
    <View
        android:id="@+id/v_pay_mask"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@color/transparent" />
</android.support.v7.widget.ContentFrameLayout>